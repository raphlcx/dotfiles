bind-key j down feedlist
bind-key k up feedlist

unbind-key j articlelist
unbind-key k articlelist
bind-key j down articlelist
bind-key k up articlelist

bind-key j down article
bind-key k up article

bind-key j down urlview
bind-key k up urlview

color info black white
color listfocus black white
color listfocus_unread black white

feedlist-format "%4i %11u %t"
articlelist-format "%4i %=8D %?T?|%-17T| ?%t"

confirm-mark-all-feeds-read no
confirm-mark-feed-read no
history-limit 0
max-items 75
refresh-on-startup yes
reload-threads 20
show-keymap-hint no
show-read-feeds no
swap-title-and-hints yes
text-width 72
